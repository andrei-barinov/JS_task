<meta charset="utf-8">
<script type="text/javascript">
    function numbers(n){
        var arr =[];
        for(var i = 0; i < n; i++){
            arr.push(i+1);
        }
        return arr
    }

    alert(numbers(5))

    function sum_arr(arr) {
        var sum = 0;
        for(var i=0; i < arr.length; i++){
           sum += arr[i];
        }
        return sum
    }

    alert(sum_arr([1, 2, 3, 4, 5]))

    function max_arr(arr) {
        var max = arr[0];
        for(var i = 1; i < arr.length; i++){
            if(arr[i] > max){
                max = arr[i];
            }
        }
        return max;
    }

    alert(max_arr([1, 23, 34, 65, 23, 6, 678, 23, 23543, 234]));

    function min_arr(arr) {
        var min = arr[0];
        for(var i = 1; i < arr.length; i++){
            if(arr[i] < min){
                min = arr[i];
            }
        }
        return min;
    }

    alert(min_arr([1, 23, 34, 65, 23, 6, -678, 23, 23543, 234]));

    function even_numbers(arr) {
        var even_numbers = [];
        for (var i = 0; i < arr.length; i++){
            if(arr[i] % 2 == 0){
                even_numbers.push(arr[i]);
            }
        }
        return even_numbers;
    }

    alert(even_numbers([2, 3, 5, 8, 10, 12, 45, 6, 3, 546, 54, 34, 11, 21, 31]));
</script>